{
  "log": {
    "level": "debug"
  },
  "bot": {
    "client": {
      "enableProxy": true,
      "timeout": "1s"
    },
    "retryTimes": 3,
    "callBackQuery": {
      "cacheTimeInSecond": 3
    }
  },
  "router": {
    "mode": "polling",
    "maxWorker": -1,
    "polling": {
      "timeoutInSecond": 60
    }
  },
  "database": {
    "url": "./data/sqlite3.db",
    "driver": "sqlite3",
    "maxIdleConns": 3,
    "maxOpenConns": 0
  },
  "fastcache": {
    "maxBytes": 2147483648
  },
  "user": {
    "accountExpiration": "5m",
    "permission": {
      "maxAccount": 3,
      "allowPolling": true,
      "timezone": 480,
      "language": "en"
    }
  },
  "language": [
    "en",
    "ja",
    "zh-CN",
    "zh-TW"
  ]
}