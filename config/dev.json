{
  "nintendo": {
    "useProxy": false
  },
  "bot": {
    "useProxy": true,
    "worker": 16,
    "debug": true
  },
  "log": {
    "level": "debug"
  },
  "db": {
    "url": "./data/sqlite3.db",
    "maxIdleConns": 3,
    "maxOpenConns": 0
  },
  "cache": {
    "fastcache": {
      "maxBytes": 2147483648
    },
    "gocache": {
      "expiration": "5m",
      "cleanUp": "10m"
    }
  },
  "account": {
    "maxAccount": 3,
    "allowPolling": true
  },
  "service": {
    "language": [
      "en",
      "zh-CN",
      "zh-TW",
      "ja"
    ],
    "callbackQueryCachedSecond": 3,
    "retryTimes": 3
  }
}