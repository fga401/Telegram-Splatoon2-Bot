{
  "nintendo": {
    "useProxy": false
  },
  "bot": {
    "useProxy": true,
    "worker": 32,
    "debug": false
  },
  "log": {
    "level": "info"
  },
  "db": {
    "url": "./data/sqlite3.db",
    "maxIdleConns": 3,
    "maxOpenConns": 0
  },
  "cache": {
    "fastcache": {
       "maxBytes": 1073741824
    },
    "gocache": {
      "expiration": "5m",
      "cleanUp": "10m"
    }
  },
  "account": {
    "maxAccount": 3,
    "allowPolling": false
  },
  "service": {
    "language": [
      "en"
    ],
    "callbackQueryCachedSecond": 3,
    "retryTimes": 5,
    "updateFailureRetryInterval": "1min"
  }
}